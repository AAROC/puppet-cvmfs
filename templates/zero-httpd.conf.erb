# Created by cvmfs_server.  Don't touch.
KeepAlive On
# Translation URL to real pathname
Alias /cvmfs/<%= @repo %> <%= @repo_store %>/<%= @repo %>
<Directory "<%= @repo_store %>/<%= @repo %>">
    Options -MultiViews
    AllowOverride Limit
    Order allow,deny
    Allow from all

    EnableMMAP Off
    EnableSendFile Off

    AddType application/x-cvmfs .cvmfspublished .cvmfswhitelist

    Header unset Last-Modified
    FileETag None

    ExpiresActive On
    ExpiresDefault "access plus 3 days"
    ExpiresByType text/html "access plus 5 minutes"
    ExpiresByType application/x-cvmfs "access plus 2 minutes"
</Directory>
